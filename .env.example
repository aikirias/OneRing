COMPOSE_PROJECT_NAME=onerdata
PROJECT_NETWORK=onerdata_net
PRIMARY_DOMAIN=localhost

# Airflow
AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW_FERNET_KEY=generate_me
AIRFLOW_EXECUTOR=CeleryExecutor
AIRFLOW_LOAD_EXAMPLES=False

# Postgres
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflowpass
POSTGRES_DB=airflow

# Analytics Postgres (Gold)
CURATED_PG_USER=curated_user
CURATED_PG_PASSWORD=curatedpass
CURATED_PG_DB=curated

# ClickHouse
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=clickpass
CLICKHOUSE_DB=analytics

# MinIO
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_CONSOLE_PORT=9001
MINIO_API_PORT=9000
MINIO_BUCKET_BRONZE=bronze
MINIO_BUCKET_SILVER=silver
MINIO_BUCKET_GOLD=gold

# Airbyte
AIRBYTE_VERSION=0.50.47
AIRBYTE_USERNAME=airbyte
AIRBYTE_PASSWORD=airbyte
AIRBYTE_API_URL=http://airbyte-server:8001/api
AIRBYTE_WORKSPACE_ROOT=/workspace
AIRBYTE_CONFIG_ROOT=/data/config
AIRBYTE_LOGS_ROOT=/data/logs
AIRBYTE_DATABASE_HOST=airbyte-db
AIRBYTE_DATABASE_PORT=5432
AIRBYTE_DATABASE_DB=airbyte
AIRBYTE_DATABASE_USER=airbyte
AIRBYTE_DATABASE_PASSWORD=airbyte
AIRBYTE_LOG_LEVEL=INFO
CONFIGS_DATABASE_MINIMUM_FLYWAY_MIGRATION_VERSION=0
JOBS_DATABASE_MINIMUM_FLYWAY_MIGRATION_VERSION=0
INTERNAL_API_HOST=airbyte-server:8001
INTERNAL_API_URL=http://airbyte-server:8001
WORKER_ENVIRONMENT=docker
LOCAL_CONNECTOR_CATALOG_PATH=
WEBAPP_URL=http://localhost:8000/
LOCAL_ROOT=/workspace
LOCAL_DOCKER_MOUNT=/workspace
WORKSPACE_ROOT=/workspace
CONFIG_ROOT=/data/config

# Infisical
# Generate 32-character secrets (e.g. python -c "import secrets;print(secrets.token_hex(16))")
INFISICAL_SERVER_URL=http://localhost:8082
INFISICAL_CLIENT_ID=replace_with_client_id
INFISICAL_CLIENT_SECRET=replace_with_client_secret
INFISICAL_WORKSPACE_ID=replace_with_workspace_id
INFISICAL_ENVIRONMENT=dev
INFISICAL_ENCRYPTION_KEY=replace_with_32_char_secret
INFISICAL_AUTH_SECRET=replace_with_32_char_secret
INFISICAL_ROOT_ENCRYPTION_KEY=replace_with_32_char_secret

# Great Expectations
GE_DATA_CONTEXT_ROOT_DIRECTORY=/opt/ge

# OpenMetadata
OPENMETADATA_DB_USER=openmetadata
OPENMETADATA_DB_PASSWORD=openmetadata
OPENMETADATA_SERVER_PORT=8585
OPENMETADATA_API_PORT=8586

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Misc
TZ=UTC
