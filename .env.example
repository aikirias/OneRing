COMPOSE_PROJECT_NAME=onerdata
PROJECT_NETWORK=onerdata_net
PRIMARY_DOMAIN=localhost

# Airflow
AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW_FERNET_KEY=generate_me
AIRFLOW_SECRET_BACKEND=infisical_backend
AIRFLOW_EXECUTOR=CeleryExecutor
AIRFLOW_LOAD_EXAMPLES=False

# Postgres
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflowpass
POSTGRES_DB=airflow

# Analytics Postgres (Gold)
CURATED_PG_USER=curated_user
CURATED_PG_PASSWORD=curatedpass
CURATED_PG_DB=curated

# ClickHouse
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=clickpass
CLICKHOUSE_DB=analytics

# MinIO
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_CONSOLE_PORT=9001
MINIO_API_PORT=9000
MINIO_BUCKET_BRONZE=bronze
MINIO_BUCKET_SILVER=silver
MINIO_BUCKET_GOLD=gold

# Airbyte
AIRBYTE_USERNAME=airbyte
AIRBYTE_PASSWORD=airbyte
AIRBYTE_API_URL=http://airbyte-server:8001/api

# Great Expectations
GE_DATA_CONTEXT_ROOT_DIRECTORY=/opt/ge

# OpenMetadata
OPENMETADATA_DB_USER=openmetadata
OPENMETADATA_DB_PASSWORD=openmetadata
OPENMETADATA_SERVER_PORT=8585
OPENMETADATA_API_PORT=8586

# Liquibase
LIQUIBASE_DEFAULT_CONTEXTS=dev
LIQUIBASE_DEFAULT_LABELS=baseline

# Infisical
INFISICAL_SERVER_URL=http://infisical:8080
INFISICAL_CLIENT_ID=replace_with_client_id
INFISICAL_CLIENT_SECRET=replace_with_client_secret
INFISICAL_WORKSPACE_ID=replace_with_workspace_id
INFISICAL_ENVIRONMENT=dev

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Misc
TZ=UTC
